{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/educacion\\/contenido(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/educacion\\/principal\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/estadisticas\\/principal\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/foros\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/usuario\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/usuario\\/registrar\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/usuario\\/registrar\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}